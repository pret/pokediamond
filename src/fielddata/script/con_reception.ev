	_TALK_CLOSE

	_BLACK_OUT		SCR_WIPE_DIV,SCR_WIPE_SYNC
	_WIPE_FADE_END_CHECK

	//シングルポケモンリスト用フラグセット
	_IFVAL_CALL		LOCALWORK3,EQ,DESK_SINGLE,ev_con_reception_pokelist_single
	//通信ポケモンリスト用フラグセット
