	_LDVAL			SCWK_ANSWER,0		/*ポケモンリスト初期カーソル位置*/
ev_con_reception_common_02_list_back:	/*ポケモンリスト画面呼び出し戻り先*/
	//ポケモン選択画面呼び出し
	_CON_POKELIST_SET_PROC	SCWK_ANSWER,SCWK_TEMP0,SCWK_TEMP1,LOCALWORK4

	//選択したポケモンの並び番号取得
	_CON_POKELIST_GET_RESULT	LOCALWORK2,SCWK_ANSWER

	_IFVAL_JUMP		SCWK_ANSWER,EQ,0,ev_con_reception_common_02_list_select_end
	_CON_POKESTATUS_SET_PROC	LOCALWORK2
	_POKESTATUS_GET_RESULT		SCWK_ANSWER
	_JUMP			ev_con_reception_common_02_list_back

ev_con_reception_common_02_list_select_end:	//リスト選択後
	_SET_MAP_PROC

	_BLACK_IN		SCR_WIPE_DIV,SCR_WIPE_SYNC
	_WIPE_FADE_END_CHECK

	//キャンセルチェック
	_IFVAL_JUMP		LOCALWORK2,EQ,0xff,ev_con_reception_end_pre	//終了確認

	_JUMP			ev_con_reception_common_02_01
	_END

