	_FLAG_SET		FV_C05R1102_CON2	//参加者を消す
	_FLAG_SET		FV_C05R1102_CON3	//参加者を消す
	_FLAG_SET		FV_C05R1102_CON4	//参加者を消す
	_FLAG_SET		FV_C05R1102_CON5	//参加者を消す
	_RET

//--------------------------------------------------------------
//	c05r1101.ev内でのFLAG_CHANGE_LABELの処理
//--------------------------------------------------------------
ev_c05r1101_flag_change:
	_POKETCH_HOOK_RESET		//ポケッチのフック解除

	_CALL			ev_c05r1102_flag_change_pofin_event	//ゴージャス兄さん
	
	_CALL			ev_con_reception_flag_clear

	_TV_INTERVIEWER_CHECK	7,LOCALWORK0
	_IFVAL_JUMP				LOCALWORK0,EQ,0,ev_c05r1101_flag_change_vanish
	_FLAG_RESET				FV_TV_INTERVIEW_07

	_END

ev_c05r1101_flag_change_vanish:
	_FLAG_SET			FV_TV_INTERVIEW_07
	_END

//--------------------------------------------------------------
//	c05r1101.ev:ゴージャス兄さん移動処理
//--------------------------------------------------------------
ev_c05r1102_flag_change_pofin_event:
	_IF_FLAGON_JUMP		FE_C05R1101_POFIN_INFOMATION,ev_c05r1102_flag_change_pofin_event_move
	_RET

ev_c05r1102_flag_change_pofin_event_move:
	_SXY_POS_CHANGE		C05R1101_GORGGEOUSM_01,24,6
	_SXY_MV_CHANGE		C05R1101_GORGGEOUSM_01,MV_DOWN
	_SXY_DIR_CHANGE		C05R1101_GORGGEOUSM_01,DIR_DOWN
	_RET


