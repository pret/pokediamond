/********************************************************************/
//
/*	通信コンテスト結果画面でセーブして、リセットした時の復帰処理	*/
//
/********************************************************************/
ev_connect_contest_return:
	_EVENT_START

	_LDVAL		SCWK_ANSWER,INFOMATION_GATE_MULTI_X
	_CALL		ev_contest_return_door

	_FLAG_RESET	SYS_FLAG_COMM_COUNTER			//通信カウンター通過フラグ(システム)
	_LDVAL		WK_SCENE_COMM_ID,SCR_COMM_ID_NOTHING

	_EVENT_END
	_END


//--------------------------------------------------------------
//	ドアから戻ってくるアニメ：共通処理
//		SCWK_ANSWER：ドア座標X
//--------------------------------------------------------------
ev_contest_return_door:
	/*どのカウンターを通過したかを覚えておいて、ドアの座標をセットする*/
	/*この強制スクリプトは必ず発動してしまってよさそう？*/
	/*出てくるアニメは共通でよさそう？*/

	_OBJ_ANIME		FLDOBJ_ID_PLAYER,anm_connect_contest_return_player_00
	_OBJ_ANIME_WAIT

	//カウンター開く
	_SET_UP_DOOR_ANIME	0,0,SCWK_ANSWER,5,77
	_CALL			connect_contest_door_open
	_OBJ_ANIME		FLDOBJ_ID_PLAYER,anm_connect_contest_return_player_02
	_OBJ_ANIME_WAIT
	_CALL			connect_contest_door_close

	_RET

//--------------------------------------------------------------
//	ドアから出てきた所からカウンターの前に戻るアニメ
//			シングル、通信で使用出来る(練習は移動距離が違う)
//		SCWK_ANSWER：ドア座標X
//--------------------------------------------------------------
ev_contest_return_door_to_counter:
	_OBJ_ANIME		FLDOBJ_ID_PLAYER,anm_connect_contest_return_player_03_2
	_OBJ_ANIME_WAIT
	_RET
	
//--------------------------------------------------------------
//	練習受付：戻りアニメ
//		引数：SCWK_TEMP0　順位
//		戻り値：SCWK_ANSWER　０＝はい、１＝いいえ
//--------------------------------------------------------------
ev_contest_return_door_practice:
	_OBJ_ANIME		FLDOBJ_ID_PLAYER,anm_connect_contest_return_player_03
	_OBJ_ANIME_WAIT
	_IFVAL_CALL		SCWK_TEMP3,EQ,CONMODE_VISUAL_PRACTICE,ev_connect_contest_practice_visual
	_IFVAL_CALL		SCWK_TEMP3,EQ,CONMODE_DANCE_PRACTICE,ev_connect_contest_practice_dance
	_IFVAL_CALL		SCWK_TEMP3,EQ,CONMODE_ACTIN_PRACTICE,ev_connect_contest_practice_actin
	
	_LDVAL		SCWK_TEMP0,CONRANK_NORMAL

	//はい・いいえ　選択
	_YES_NO_WIN		SCWK_ANSWER
	_RET

//練習受付：戻りアニメ：ビジュアル
ev_connect_contest_practice_visual:
	_IFVAL_JUMP		SCWK_TEMP0,GT,1,ev_connect_contest_practice_visual_low
	_TALK_KEYWAIT	msg_prac_v_06
	_RET
	
ev_connect_contest_practice_visual_low:	//順位が低かった
	_TALK_KEYWAIT	msg_prac_v_07
	_RET

//練習受付：戻りアニメ：ダンス
ev_connect_contest_practice_dance:
	_IFVAL_JUMP		SCWK_TEMP0,GT,1,ev_connect_contest_practice_dance_low
	_TALK_KEYWAIT	msg_prac_d_06
	_RET

ev_connect_contest_practice_dance_low:	//順位が低かった
	_TALK_KEYWAIT	msg_prac_d_07
	_RET

//練習受付：戻りアニメ：演技
ev_connect_contest_practice_actin:
	_IFVAL_JUMP		SCWK_TEMP0,GT,1,ev_connect_contest_practice_actin_low
	_TALK_KEYWAIT	msg_prac_a_06
	_RET
	
ev_connect_contest_practice_actin_low:	//順位が低かった
	_TALK_KEYWAIT	msg_prac_a_07
	_RET


//--------------------------------------------------------------
//	ドアアニメ
//--------------------------------------------------------------
connect_contest_door_open:
	_SEQ_OPEN_DOOR	77	//ドア開ける
	_WAIT_3D_ANIME	77	//アニメ終了待ち
	_RET

connect_contest_door_close:
	_SEQ_CLOSE_DOOR	77	//ドア閉める
	_WAIT_3D_ANIME	77	//アニメ終了待ち
	_FREE_3D_ANIME	77	//ドアアニメ解放
	_RET

