	_JUMP			common_d03r0101_leader
	_END

ev_d03r0101_leader_dir_right:
	_OBJ_ANIME		D03R0101_LEADER,anm_d03r0101_leader_11
	_OBJ_ANIME_WAIT
	_JUMP			common_d03r0101_leader
	_END

common_d03r0101_leader:
	_OBJ_DEL		D03R0101_LEADER
	_TALK_OBJ_END
	_END

