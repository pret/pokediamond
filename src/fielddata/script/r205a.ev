	_OBJ_ANIME		R205A_BABYGIRL1,anm_r205a_babygirl1_02
	_OBJ_ANIME_WAIT

	_OBJ_ANIME		FLDOBJ_ID_PLAYER,anm_r205a_player_01
	_OBJ_ANIME_WAIT
