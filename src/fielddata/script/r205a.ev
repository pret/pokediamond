/*								少女 								*/
/********************************************************************/
ev_r205a_babygirl1:
	_TALK_OBJ_START

	//会話していたら
	_IF_FLAGON_JUMP	FE_R205A_BABYGIRL1_TALK,ev_r205a_babygirl1_02

	_CALL			ev_r205a_babygirl1_flag_set

	_TALKMSG		msg_r205a_babygirl1_01
	_LAST_KEYWAIT
	_TALK_CLOSE
	_TALK_OBJ_END
	_END

//会話していたら
ev_r205a_babygirl1_02:
	_TALKMSG		msg_r205a_babygirl1_02
	_LAST_KEYWAIT
	_TALK_CLOSE
	_TALK_OBJ_END
	_END

//フラグセット
