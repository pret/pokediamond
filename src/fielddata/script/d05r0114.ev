	_SE_PLAY		SEQ_SE_DP_W025

	_TIME_WAIT		90, SCWK_ANSWER

	//ボス「おお！　○○」
	_LDVAL_VERSION	msg_d05r0114_gingaboss_05, msg_d05r0115_gingaboss_05
