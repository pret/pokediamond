	_OBJ_ANIME		D05R0114_SUPPORT, anm_d05r0114_frameout
	_OBJ_ANIME		FLDOBJ_ID_PLAYER, anm_d05r0114_frameout
	_OBJ_ANIME_WAIT

	_OBJ_DIR_CHANGE	D05R0114_DOCTOR, DIR_DOWN
	_TALKMSG		msg_d05r0114_doctor_02
	_AB_KEYWAIT
	_TALK_CLOSE

	_BLACK_OUT		SCR_WIPE_DIV,SCR_WIPE_SYNC
	_WIPE_FADE_END_CHECK

	/*画面暗転、入り口付近に飛ばして操作可能に*/
	_MAP_CHANGE_NONE_FADE	ZONE_ID_D05R0114,0,31,52,DIR_DOWN

	//ナナカマド＆サポートを入り口へ移動
	_OBJ_POS_CHANGE		D05R0114_SUPPORT,32, 10, 51, DIR_DOWN
	_OBJ_POS_CHANGE		D05R0114_DOCTOR, 30, 10, 51, DIR_DOWN

	//アイ/ハイ/レイそれぞれ湖に出現
	_FLAG_RESET		FV_D27R0103_AI
	_FLAG_RESET		FV_D28R0103_HAI
	_FLAG_RESET		FV_D29R0103_REI

	//バトルサーチャーLv.3
	_FLAG_SET		SYS_FLAG_BS_LV3
	//ナギサシティのストッパー解除
	_FLAG_SET		FV_R222_WORKMAN

	/*アジトにサターン出現*/
	_FLAG_RESET		FV_D26R0101_GKANBU

	//やりのはしらイベントクリア！
	_FLAG_SET		FE_D05R0114_CLEAR
	_FLAG_SET		FV_D05R0112_GINGAW		/*ギンガ団消す*/

	//ストッパーのしたっぱを消す
	_FLAG_SET		FV_D05R0110_GINGAM

	_LDVAL			WK_SCENE_D05R0114,SCENE_D05R0114_CLEAR

	_BLACK_IN		SCR_WIPE_DIV,SCR_WIPE_SYNC
	_WIPE_FADE_END_CHECK

	_EVENT_END
	_END

