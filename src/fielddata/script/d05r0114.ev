/********************************************************************/
/*						FLAG_CHANGE_LABEL 							*/
/********************************************************************/
ev_d05r0114_flag_change:
	_ARRIVE_FLAG_SET		FLAG_ARRIVE_D05R0114	//到着

	_CALL		d05r0114_flag_change_scene

	_CALL		sub_d05r0114_flag_change_mons

	_CALL		sub_d05r0114_flag_change_itemball

	_CALL		sub_d05r0114_flag_change_tenkainofue

	//WKOBJCHRにサポートのキャラコードをセットする
	_GET_MY_SEX		LOCALWORK0
	_IFVAL_JUMP		LOCALWORK0,EQ,0,d05r0114_male_flag_change_01	//サポート女
	_IFVAL_JUMP		LOCALWORK0,EQ,1,d05r0114_female_flag_change_01	//サポート男
	_END

d05r0114_male_flag_change_01:	//サポート女
	_LDVAL			OBJCHRWORK0,HEROINE
	_END

d05r0114_female_flag_change_01:	//サポート男
	_LDVAL			OBJCHRWORK0,HERO
	_END

//--------------------------------------------------------------------
//OBJCHRWORK1にイア・エアどちらかをセットする
//--------------------------------------------------------------------
sub_d05r0114_flag_change_mons:
	_LDVAL				OBJCHRWORK1,SPPOKE4		//ディアルガ（イア）

	_PM_VERSION_GET		LOCALWORK0
	_IFVAL_JUMP			LOCALWORK0,EQ,VERSION_DIAMOND, sub_d05r0114_flag_change_mons_ret

	_LDVAL				OBJCHRWORK1,SPPOKE5		//パルキア（エア）
sub_d05r0114_flag_change_mons_ret:
	_RET

//--------------------------------------------------------------------
//	バージョンによって出現するたまを制限
//--------------------------------------------------------------------
sub_d05r0114_flag_change_itemball:
	_PM_VERSION_GET		LOCALWORK0
	_IFVAL_JUMP			LOCALWORK0,EQ,VERSION_DIAMOND, sub_d05r0114_flag_change_itemball_diamond

	_FLAG_SET			FV_FLD_ITEM_37	//こんごうだま消去
	_RET

sub_d05r0114_flag_change_itemball_diamond:
	_FLAG_SET			FV_FLD_ITEM_39	//しらたま消去
	_RET

//--------------------------------------------------------------------
//	てんかいのふえのPOSを使えるようにするかどうかの設定
//--------------------------------------------------------------------
sub_d05r0114_flag_change_tenkainofue:
/* てんかいのふえが使える条件 */
	/*ゲームクリア？*/
	_SYS_FLAG_GAME_CLEAR_GET	LOCALWORK0
	_IFVAL_JUMP					LOCALWORK0,EQ,0, sub_d05r0114_flag_change_tenkainofue_end
	/* 全国図鑑入手後？*/
	_ZENKOKU_ZUKAN_FLAG			FLAG_MODE_GET, LOCALWORK0
	_IFVAL_JUMP					LOCALWORK0,EQ,0, sub_d05r0114_flag_change_tenkainofue_end
	/* てんかいのふえをもっている？*/
	_ITEM_CHK					ITEM_TENKAINOHUE,1, LOCALWORK0
	_IFVAL_JUMP					LOCALWORK0,EQ,0, sub_d05r0114_flag_change_tenkainofue_end
	/* 配布用マジックナンバー正常？*/
	_IS_HAIHU_EVENT_ENABLE		HAIHUEVENT_ID_D05, LOCALWORK0
	_IFVAL_JUMP					LOCALWORK0,EQ,0, sub_d05r0114_flag_change_tenkainofue_end
	/* アウス（アルセウス）を捕まえていない？*/
	_IF_FLAGON_JUMP				FE_D05R0116_LEGEND_CLEAR, sub_d05r0114_flag_change_tenkainofue_end

	_LDVAL						WK_SCENE_D05R0116, 1

sub_d05r0114_flag_change_tenkainofue_end:
	_RET

//--------------------------------------------------------------------
//--------------------------------------------------------------------
d05r0114_flag_change_scene:
	_DEBUG_WATCH_WORK	WK_SCENE_D05R0114
	_IFVAL_JUMP	WK_SCENE_D05R0114,EQ,SCENE_D05R0114_START,			d05r0114_flag_change_scene_00
	_IFVAL_JUMP	WK_SCENE_D05R0114,EQ,SCENE_D05R0114_SPPOKE_DEMO,	d05r0114_flag_change_scene_01
	_IFVAL_JUMP	WK_SCENE_D05R0114,EQ,SCENE_D05R0114_GINGA_BATTLE,	d05r0114_flag_change_scene_02
	_IFVAL_JUMP	WK_SCENE_D05R0114,EQ,SCENE_D05R0114_SPPOKE_BATTLE,	d05r0114_flag_change_scene_03
	_IFVAL_JUMP	WK_SCENE_D05R0114,EQ,SCENE_D05R0114_CLEAR,			d05r0114_flag_change_scene_04
	_IFVAL_JUMP	WK_SCENE_D05R0114,EQ,SCENE_D05R0114_SPPOKE_RETRY,	d05r0114_flag_change_scene_05
	_RET

d05r0114_flag_change_scene_00:
d05r0114_flag_change_scene_01:
	_FLAG_SET	FV_D05R0114_SUPPORT
	_FLAG_SET	FV_D05R0114_DOCTOR
	_FLAG_SET	FV_D05R0114_RIVAL
	_FLAG_SET	FV_D05R0114_POKE_L01
	_FLAG_SET	FV_D05R0114_POKE_L02
	_FLAG_SET	FV_D05R0114_POKE_L03
	_RET
d05r0114_flag_change_scene_02:
	_RET
d05r0114_flag_change_scene_03:
	_SXY_POS_CHANGE		D05R0114_SUPPORT, 30, SUPPORT_POS_Z0
	_SXY_POS_CHANGE		D05R0114_DOCTOR,  32, DOCTOR_POS_Z0
	_RET

d05r0114_flag_change_scene_04:
	_FLAG_SET	FV_D05R0114_SUPPORT
	_FLAG_SET	FV_D05R0114_DOCTOR
	_FLAG_SET	FV_D05R0114_RIVAL

	_RET

d05r0114_flag_change_scene_05:
	_FLAG_SET	FV_D05R0114_SUPPORT
	_FLAG_SET	FV_D05R0114_DOCTOR
	_FLAG_RESET			FV_D05R0114_SPPOKE_01
	_RET

/********************************************************************/
/*						INIT_CHANGE_LABEL 							*/
