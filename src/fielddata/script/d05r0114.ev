	_ANIME_DATA	ACMD_END,0

_ANIME_LABEL	anm_d05r0114_look_gingam_02
	_ANIME_DATA	AC_STAY_WALK_L_8F,1
	_ANIME_DATA	ACMD_END,0

_ANIME_LABEL	anm_d05r0114_default_gingam_02
	_ANIME_DATA AC_STAY_WALK_D_8F,1
	_ANIME_DATA	ACMD_END,0


//一歩戻される
_ANIME_LABEL	anm_d05r0114_return_player_01
	_ANIME_DATA	AC_WAIT_4F,5
	_ANIME_DATA AC_WALK_D_8F,1
	_ANIME_DATA	ACMD_END,0
	
/********************************************************************/
/*		ギンガ団ザコ:話しかけ										*/
/********************************************************************/
ev_d05r0114_gingam:
	//_EVENT_START
	_TALK_OBJ_START

	_IFVAL_JUMP	WK_SCENE_D05R0114,NE,SCENE_D05R0114_START,ev_d05r0114_gingam_battle_after

	_2VS2_BATTLE_CHECK	SCWK_ANSWER
	_IFVAL_JUMP	SCWK_ANSWER,EQ,0,ev_d05r0114_gingam_not_battle

//ギンガ団ザコ対戦（ダブルバトル）へ
ev_d04r0114_gingam_battle:
	_TALKMSG		msg_d05r0114_gingam_01
	_TALK_CLOSE

	_TRAINER_BTL_SET	ZAKO1_TRAINER_ID, ZAKO2_TRAINER_ID
	_TRAINER_LOSE_CHECK	SCWK_ANSWER
	_IFVAL_JUMP		SCWK_ANSWER,EQ,0,ev_d05r0114_gingam_battle_lose

	_LDVAL			WK_SCENE_D05R0114,SCENE_D05R0114_SPPOKE_DEMO
	_TALKMSG		msg_d05r0114_gingam_03
	_LAST_KEYWAIT
	_TALK_CLOSE
	_EVENT_END
	_END

//ポケモンを2体持っていないのでダブルバトルできない
ev_d05r0114_gingam_not_battle:
	_TALKMSG		msg_d05r0114_gingam_02
	_TALK_CLOSE
	_EVENT_END
	_END

//ギンガ団ザコに敗北した
ev_d05r0114_gingam_battle_lose:
	_TRAINER_LOSE
	_EVENT_END
	_END

//ギンガ団ザコ勝利後
ev_d05r0114_gingam_battle_after:
	_TALKMSG		msg_d05r0114_gingam_03
	_LAST_KEYWAIT
	_TALK_CLOSE
	_EVENT_END
	_END


//============================================================================================
//
//
//		シーン１：ボスの台詞〜イア（エア）召還まで
//		SCENE_D05R0114_SPPOKE_DEMO
//
//============================================================================================
/********************************************************************/
/*		ギンガ団ボス:イア召還										*/
/********************************************************************/
pos_d05r0114_gingaboss:
	_EVENT_START
	_OBJ_INVISIBLE	D05R0114_SPPOKE_01
	_OBJ_PAUSE_CLEAR D05R0114_SPPOKE_01		// イアの動さ一時停止は解除します tomoya
//	_FLAG_RESET		FV_D05R0114_SPPOKE_01
//	_OBJ_ADD		D05R0114_SPPOKE_01

	//カメラ移動、ボス中心へ
	_PLAYER_POS_GET			SCWK_PARAM0,SCWK_PARAM1
	_VANISH_DUMMY_OBJ_ADD	SCWK_PARAM0,SCWK_PARAM1
	_CALL				sub_d05r0114_camera_jiki2boss
	//_OBJ_ANIME		SCR_OBJID_DUMMY,anm_d05r0114_camera_01
	_OBJ_ANIME_WAIT

	_LDVAL_VERSION		msg_d05r0114_gingaboss_01,msg_d05r0115_gingaboss_01
	_TALKMSG_SP			SCWK_TEMP0
	_TALK_CLOSE

	_BGM_FADEOUT	0, 60

	_OBJ_ANIME	D05R0114_GINGABOSS,anm_d05r0114_boss_01
	_OBJ_ANIME_WAIT

	//カメラ移動、イア中心へ
	_OBJ_ANIME		SCR_OBJID_DUMMY,anm_d05r0114_camera_02
	_OBJ_ANIME_WAIT

	//黒い球からイア出現のデモ

	//_BGM_STOP		SEQ_D_MOUNT2
	//_BGM_PLAY		SEQ_THE_EVENT02

	//サウンドロード
	_SND_CLIMAX_DATA_LOAD

	_CLIMAX_DEMO	CLIMAX_DEMO_LEGENDPOKE_START,SCWK_ANSWER
	_TIME_WAIT		30*5 + 12, SCWK_ANSWER

	/*サブ画面サウンドデータを消さないように、PLAYER_FIELDで再生するようにした！*/
	_BGM_PLAY		SEQ_THE_EVENT02

//	_TIME_WAIT		35*30, SCWK_ANSWER
demo_loop:
	_CLIMAX_DEMO	CLIMAX_DEMO_LEGENDPOKE_ENDCHK,SCWK_ANSWER
	_IFVAL_JUMP		SCWK_ANSWER,EQ,0,demo_loop

//	_CLIMAX_DEMO	CLIMAX_DEMO_LEGENDPOKE_ENDCHK,SCWK_ANSWER
	_CLIMAX_DEMO	CLIMAX_DEMO_BLACKRING_START,SCWK_ANSWER
	_OBJ_ANIME		SCR_OBJID_DUMMY,anm_d05r0114_camera_03
	_OBJ_ANIME_WAIT

	//赤い鎖どうする？
	//イア登場デモ
	_LDVAL_VERSION		msg_d05r0114_gingaboss_02, msg_d05r0115_gingaboss_02
	_TALKMSG_SP			SCWK_TEMP0

	_CTRL_BGM_FLAG_SET			//BGM固定の開始
	_FLD_SCOPE_MODE_ON			//望遠鏡モード：フラグ更新禁止

	_BLACK_OUT		SCR_WIPE_DIV,SCR_WIPE_SYNC
	_WIPE_FADE_END_CHECK
	_VANISH_DUMMY_OBJ_DEL
	_TALK_CLOSE
	//画面切替：やりのはしら上空
	_MAP_CHANGE_NONE_FADE		ZONE_ID_D05R0114,0,31,25,DIR_RIGHT
	_OBJ_INVISIBLE	FLDOBJ_ID_PLAYER
	_CLIMAX_DEMO	CLIMAX_DEMO_ANTEGG_00,SCWK_ANSWER
	_BLACK_IN		SCR_WIPE_DIV,SCR_WIPE_SYNC
	_BGM_PLAY			SEQ_THE_EVENT03
	_WIPE_FADE_END_CHECK
	_TIME_WAIT		90,SCWK_ANSWER
	_BLACK_OUT		SCR_WIPE_DIV,SCR_WIPE_SYNC
	_WIPE_FADE_END_CHECK
	//画面切替：風力発電所
	_MAP_CHANGE_NONE_FADE		ZONE_ID_D02,0,240,660,DIR_RIGHT
	_OBJ_INVISIBLE	FLDOBJ_ID_PLAYER
	_CLIMAX_DEMO	CLIMAX_DEMO_ANTEGG_01,SCWK_ANSWER
	_BLACK_IN		SCR_WIPE_DIV,SCR_WIPE_SYNC
	_WIPE_FADE_END_CHECK
	_TIME_WAIT		90,SCWK_ANSWER
	_BLACK_OUT		SCR_WIPE_DIV,SCR_WIPE_SYNC
	_WIPE_FADE_END_CHECK
	//画面切替：コンテスト会場
	_MAP_CHANGE_NONE_FADE		ZONE_ID_C05,0,479,692,DIR_LEFT
	_OBJ_INVISIBLE	FLDOBJ_ID_PLAYER
	_CLIMAX_DEMO	CLIMAX_DEMO_ANTEGG_02,SCWK_ANSWER
	_BLACK_IN		SCR_WIPE_DIV,SCR_WIPE_SYNC
	_WIPE_FADE_END_CHECK
	_TIME_WAIT		90,SCWK_ANSWER
	_BLACK_OUT		SCR_WIPE_DIV,SCR_WIPE_SYNC
	_WIPE_FADE_END_CHECK
	//元のマップへ
	/*黒い輪の表示をセット*/
	_FLAG_SET		FE_D05R0114_BLACKRING
	_MAP_CHANGE_NONE_FADE		ZONE_ID_D05R0114,0,SCWK_PARAM0,SCWK_PARAM1,DIR_UP
	_OBJ_VISIBLE	FLDOBJ_ID_PLAYER
	_VANISH_DUMMY_OBJ_ADD	SCWK_PARAM0,SCWK_PARAM1
	_CALL				sub_d05r0114_camera_jiki2boss
	_OBJ_ANIME_WAIT
	_BLACK_IN		SCR_WIPE_DIV,SCR_WIPE_SYNC
	_WIPE_FADE_END_CHECK

	_CTRL_BGM_FLAG_RESET		//BGM固定の解除
	_FLD_SCOPE_MODE_OFF			//望遠鏡モード：フラグ更新禁止の解除


	_LDVAL_VERSION		msg_d05r0114_gingaboss_03,msg_d05r0115_gingaboss_03
	_TALKMSG_SP			SCWK_TEMP0
	_TALK_CLOSE

	//カメラ移動、イア中心へ
	_OBJ_ANIME		SCR_OBJID_DUMMY,anm_d05r0114_camera_02
	_OBJ_ANIME_WAIT

	_LDVAL_VERSION		MONSNO_IA, MONSNO_EA
	_VOICE_PLAY			SCWK_TEMP0, 0
	_LDVAL_VERSION		msg_d05r0114_sppoke4_01, msg_d05r0115_sppoke5_01
	_TALKMSG_SP			SCWK_TEMP0
	_VOICE_WAIT
	_TALK_CLOSE

	//カメラ移動、ボス中心へ
	_OBJ_ANIME		SCR_OBJID_DUMMY,anm_d05r0114_camera_03
	_OBJ_ANIME_WAIT

	_TALKMSG	msg_d05r0114_gingaboss_04
	_AB_KEYWAIT
	_TALK_CLOSE

	_CALL				sub_d05r0114_camera_boss2jiki
	_OBJ_ANIME_WAIT
	_VANISH_DUMMY_OBJ_DEL

	_LDVAL		WK_SCENE_D05R0114,SCENE_D05R0114_GINGA_BATTLE
	_EVENT_END
	_END

