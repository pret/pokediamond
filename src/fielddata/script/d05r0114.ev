ev_d05r0114_init_change:
	_IF_FLAGOFF_JUMP	FE_D05R0114_BLACKRING,ev_d05r0114_init_change_ia_check

	_CLIMAX_DEMO	CLIMAX_DEMO_BLACKRING_START,LOCALWORK0

ev_d05r0114_init_change_ia_check:
