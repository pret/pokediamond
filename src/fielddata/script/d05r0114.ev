	_PLAYER_POS_GET			SCWK_TEMP0,SCWK_TEMP1
	_VANISH_DUMMY_OBJ_ADD	SCWK_TEMP0,SCWK_TEMP1
	//サポートが主人公を連れて去る

	_OBJ_ANIME		D05R0114_SUPPORT, anm_d05r0114_support_03
