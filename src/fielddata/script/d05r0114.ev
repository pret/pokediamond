	_TALK_CLOSE

	_TIME_WAIT		60, SCWK_ANSWER

	//_BGM_STOP		SEQ_D_MOUNT2
	_BGM_PLAY		SEQ_D_RYAYHY

	_TIME_WAIT		90, SCWK_ANSWER

	//ボス「……なんだ！？」
