	_IF_FLAGOFF_CALL	FE_D24R0105_SEVEN3_TALK,ev_d24r0105_seven3_01_talk_off
	_IF_FLAGON_CALL		FE_D24R0105_SEVEN3_TALK,ev_d24r0105_seven3_01_talk_on

	_PLAYER_NAME	0
	_ME_PLAY		SEQ_GONIN
	_TALKMSG		msg_d24r0105_pair_01
