	_FLAG_SET		FE_D24R0105_SEVEN3_POKE_MAX

	/*５人衆イベントクリア*/
	//_CALL			ev_d24r0105_seven3_clear
	/*バトルタワーに出てくるかフラグはまだオンにしない！*/
	_LDVAL			WK_SCENE_D24R0105,2

	_TALKMSG		msg_d24r0105_seven3_08
	_LAST_KEYWAIT
	_TALK_CLOSE
	_RET

