ev_d24r0105_seven3_01:
	_EVENT_START

	_BICYCLE_REQ		0	/*強制的に自転車から降りるようにする*/

	//主人公の位置情報取得
	_PLAYER_POS_GET	SCWK_TEMP0,SCWK_TEMP1
