sub_c07_get_support_id:
	_GET_MY_SEX		SCWK_ANSWER
	_IFVAL_JUMP		SCWK_ANSWER,EQ,0,c07_male_100			//サポート女
	_IFVAL_JUMP		SCWK_ANSWER,EQ,1,c07_female_100			//サポート男
	_END
	
c07_male_100:			//サポート女
	_FIRST_POKE_NO_GET	SCWK_ANSWER

	_LDVAL				SCWK_TEMP0,HEROINE_04
	_IFVAL_JUMP			SCWK_ANSWER,EQ,MONSNO_HIKOZARU,sub_c07_get_support_id_ret

	_LDVAL				SCWK_TEMP0,HEROINE_05
	_IFVAL_JUMP			SCWK_ANSWER,EQ,MONSNO_POTTYAMA,sub_c07_get_support_id_ret

	_LDVAL				SCWK_TEMP0,HEROINE_06
	_RET

c07_female_100:			//サポート男
	_FIRST_POKE_NO_GET	SCWK_ANSWER

	_LDVAL				SCWK_TEMP0,HERO_04
	_IFVAL_JUMP			SCWK_ANSWER,EQ,MONSNO_HIKOZARU,sub_c07_get_support_id_ret

	_LDVAL				SCWK_TEMP0,HERO_05
	_IFVAL_JUMP			SCWK_ANSWER,EQ,MONSNO_POTTYAMA,sub_c07_get_support_id_ret

	_LDVAL				SCWK_TEMP0,HERO_06
	_RET

