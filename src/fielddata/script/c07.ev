//--------------------------------------------------------------------
/*共通*/
//--------------------------------------------------------------------
common_c07_support_50:
	_TALK_CLOSE

	_PLAYER_DIR_GET	SCWK_ANSWER
	_IFVAL_CALL		SCWK_ANSWER,EQ,DIR_RIGHT,ev_c07_support_right
	_IFVAL_CALL		SCWK_ANSWER,EQ,DIR_DOWN,ev_c07_support_down
	_IFVAL_CALL		SCWK_ANSWER,EQ,DIR_UP,ev_c07_support_up

	//戦闘呼び出し
	_CALL			sub_c07_get_support_id
	_TRAINER_MULTI_BTL_SET	SCWK_TEMP0,GINGAM_37,GINGAM_38
	_TRAINER_LOSE_CHECK SCWK_ANSWER
	_IFVAL_JUMP			SCWK_ANSWER,EQ,0,ev_c07_common_battle_lose

	/*ギンガ団*/
	_TALK_KEYWAIT	msg_c07_gingam_1_03

	/*主人公の位置で分岐 動く*/
	_PLAYER_POS_GET	SCWK_TEMP0,SCWK_TEMP1
	_IFVAL_CALL		SCWK_TEMP1,EQ,POS_C07_SUPPORT_Z1,ev_c07_support_pos_up_01
	_IFVAL_CALL		SCWK_TEMP1,EQ,POS_C07_SUPPORT_Z2,ev_c07_support_pos_down_01

	_TALK_KEYWAIT	msg_c07_gingam_1_04
	_TALK_CLOSE

	_TIME_WAIT		15,SCWK_ANSWER

	/*一人目退場*/
	_PLAYER_POS_GET	SCWK_TEMP0,SCWK_TEMP1
	_IFVAL_CALL		SCWK_TEMP1,EQ,POS_C07_SUPPORT_Z1,ev_c07_support_pos_up
	_IFVAL_CALL		SCWK_TEMP1,EQ,POS_C07_SUPPORT_Z2,ev_c07_support_pos_down
	_OBJ_DEL		C07_GINGAM_1

	_TALK_KEYWAIT	msg_c07_gingam_2_03
	_TALK_CLOSE

	/*二人目退場*/
	_PLAYER_POS_GET	SCWK_TEMP0,SCWK_TEMP1
	_IFVAL_CALL		SCWK_TEMP1,EQ,POS_C07_SUPPORT_Z1,ev_c07_support_pos_up_02
	_IFVAL_CALL		SCWK_TEMP1,EQ,POS_C07_SUPPORT_Z2,ev_c07_support_pos_down_02
	_OBJ_DEL		C07_GINGAM_2

	/*ギンガ団が去った方を見ながら*/
	_GET_MY_SEX		SCWK_TEMP0
	_IFVAL_CALL		SCWK_TEMP0,EQ,0,c07_support_male_30
	_IFVAL_CALL		SCWK_TEMP0,EQ,1,c07_support_female_30
	_TALK_CLOSE

	/*主人公と向き合って*/
	_PLAYER_POS_GET	SCWK_TEMP0,SCWK_TEMP1
	_IFVAL_CALL		SCWK_TEMP1,EQ,POS_C07_SUPPORT_Z1,ev_c07_support_pos_up_03
	_IFVAL_CALL		SCWK_TEMP1,EQ,POS_C07_SUPPORT_Z2,ev_c07_support_pos_down_03

	_TIME_WAIT		10,SCWK_ANSWER

	_PLAYER_NAME	0
	_GET_MY_SEX		SCWK_TEMP0
	_IFVAL_CALL		SCWK_TEMP0,EQ,0,c07_support_male_40
	_IFVAL_CALL		SCWK_TEMP0,EQ,1,c07_support_female_40
	_TALK_CLOSE

	_TIME_WAIT		15,SCWK_ANSWER

	/*退場*/
	_OBJ_ANIME		C07_SUPPORT,anm_c07_support_70
	_OBJ_ANIME		FLDOBJ_ID_PLAYER,anm_c07_player_70
	_OBJ_ANIME_WAIT
	_JUMP			common_c07_support_end

