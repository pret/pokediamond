#define POS_C07_SUPPORT_GYM_X1	(681)
#define POS_C07_SUPPORT_GYM_X2	(682)
#define POS_C07_SUPPORT_GYM_X3	(683)
#define POS_C07_SUPPORT_GYM_X4	(684)

pos_c07_support:
	_EVENT_START

	/*「！」*/
	_OBJ_ANIME		C07_SUPPORT,anm_c07_support_200
	_OBJ_ANIME_WAIT

	_CHG_COMMON_SCR SCRID_THE_BOY_GIRL_PLAY				/*サポート曲再生*/

	/*近づく*/
	_PLAYER_POS_GET	SCWK_TEMP0,SCWK_TEMP1
	_IFVAL_CALL		SCWK_TEMP0,EQ,POS_C07_SUPPORT_GYM_X1,ev_c07_support_gym_pos_01
	_IFVAL_CALL		SCWK_TEMP0,EQ,POS_C07_SUPPORT_GYM_X2,ev_c07_support_gym_pos_02
	_IFVAL_CALL		SCWK_TEMP0,EQ,POS_C07_SUPPORT_GYM_X3,ev_c07_support_gym_pos_03
	_IFVAL_CALL		SCWK_TEMP0,EQ,POS_C07_SUPPORT_GYM_X4,ev_c07_support_gym_pos_04

	_GET_MY_SEX		SCWK_TEMP0
	_IFVAL_CALL		SCWK_TEMP0,EQ,0,c07_support_male_100
	_IFVAL_CALL		SCWK_TEMP0,EQ,1,c07_support_female_100

	_TALK_CLOSE

	/*去る*/
	_PLAYER_POS_GET	SCWK_TEMP0,SCWK_TEMP1
	_IFVAL_CALL		SCWK_TEMP0,EQ,POS_C07_SUPPORT_GYM_X1,ev_c07_support_gym_pos_11
	_IFVAL_CALL		SCWK_TEMP0,EQ,POS_C07_SUPPORT_GYM_X2,ev_c07_support_gym_pos_12
	_IFVAL_CALL		SCWK_TEMP0,EQ,POS_C07_SUPPORT_GYM_X3,ev_c07_support_gym_pos_13
	_IFVAL_CALL		SCWK_TEMP0,EQ,POS_C07_SUPPORT_GYM_X4,ev_c07_support_gym_pos_14

	_OBJ_DEL		C07_SUPPORT

	_CHG_COMMON_SCR SCRID_THE_BOY_GIRL_STOP				/*サポート曲停止*/

	_LDVAL			WK_SCENE_C07_SUPPORT,1
	_EVENT_END
	_END

